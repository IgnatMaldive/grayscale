{
    "version":2,
    "builds": [
        {
            "src":"/api/index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.9"}
        }
    ],
    "routes": [
        {
            "src":"/",
            "dest":"/"
        }
    ]
}



{
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index" }
  ]
}
